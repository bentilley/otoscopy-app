{
  "__tests__/*": {
    "type": "test",
    "alternate": ["{}"]
  },
  "App.tsx": {
    "type": "app",
    "alternate": ["__tests__/App.tsx"]
  },
  "**/__tests__/*": {
    "type": "test",
    "alternate": ["{dirname}/{basename}"]
  },
  "components/*": {
    "type": "component",
    "alternate": ["components/{dirname}/__tests__/{basename}"]
  },
  "components/screens/*Screen/index.tsx": {
    "type": "screen",
    "alternate": ["components/{}/index.tsx"]
  }
}
